NEXT_PUBLIC_VERSION_NUMBER=1
NEXT_PUBLIC_SERVER_URL=http://localhost:5000
NEXT_PUBLIC_ORIGIN_DOMAIN=http://localhost:3000
NEXT_PUBLIC_MAILER_URL=http://localhost:8000